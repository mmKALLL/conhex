{
  "extends": "./node_modules/gts/",
  "env": {
    "browser": true
  },
  "rules": {
    "semi": "off"
  },
  "multiline": {
    "delimiter": "none",
    "requireLast": true
  },
  "singleline": {
    "delimiter": "semi",
    "requireLast": false
  },
  "ignorePatterns": ["**/node_modules", "build/", "dist/", "release/"]
}
