    "bugs": {
        "url": "https://github.com/mmKALLL/conhex/issues"
    },
    "homepage": "https://github.com/mmKALLL/conhex#readme",
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@types/node": "^11.11.5",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.7",
        "acorn": "6.4.1",
        "parcel-bundler": "^1.9.4",
        "sass": "^1.35.1",
        "typescript": "^4.2.2"
    }
}
